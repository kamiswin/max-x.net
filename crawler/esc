# OUT: 			function getMousePoint(ev) {
# OUT: 				var point = {x:0,y:0};
# OUT: 				if(typeof window.pageYOffset != 'undefined') {
# OUT: 					point.x = window.pageXOffset;
# OUT: 					point.y = window.pageYOffset;
# OUT: 				}
# OUT: 				else if(typeof document.compatMode != 'undefined' && document.compatMode != 'BackCompat') {
# OUT: 					point.x = document.documentElement.scrollLeft;
# OUT: 					point.y = document.documentElement.scrollTop;
# OUT: 				}
# OUT: 				else if(typeof document.body != 'undefined') {
# OUT: 					point.x = document.body.scrollLeft;
# OUT: 					point.y = document.body.scrollTop;
# OUT: 				}
# OUT: 				point.x += ev.clientX;
# OUT: 				point.y += ev.clientY;
# OUT: 				return point;
# OUT: 			}
