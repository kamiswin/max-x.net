import lxml.html,requests
from lxml.html.clean import Cleaner



cleaner = Cleaner(style=True,scripts=True,page_structure=False,safe_attrs_only=True,safe_attrs=['class'])



page = requests.get('http://drive.chinaluxus.com/Car/')
html = lxml.html.fromstring(page.content)


